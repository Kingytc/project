<sql>
	<query code="getoslimit_grpwise">
		insert into indgrpexpdetails (ind_oslimit,ind_sanclimit,ind_grp)
		(select sum(cbs_exposuredetails.cbs_outstandingamt),
		sum(cbs_exposuredetails.cbs_sanclimit),
		industry_master.ind_grpcode from industry_master,
		cbs_exposuredetails where industry_master.ind_bsr_code=
		cbs_exposuredetails.cbs_code group by
		industry_master.ind_grpcode)
	</query>
	
