		  %>
				document.write("<tr>");
				document.write("<td align='center'>");
					document.write("<div align='center'>"+"<%=(i+1)%>" +'&nbsp;'+ "</div>");
					document.write("</td>");
		   <%	for(int j=0;j<ht.size();j++)
			    {
					String dt = (String)ht.get(j);
					align =(String)ht.get(j+1);
					if (dt.equals(""))
					{
						dt="&nbsp;";
					}
					
			%>		
					document.write("<td align='<%=align%>'>");
